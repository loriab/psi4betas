#! OMP2 cc-pVDZ gradient for the H2O molecule.

ref_vals = [                                                             #TEST
             [ 0.000000000000,     0.000000000000,     0.013509658760],  #TEST
             [ 0.000000000000,     0.002462822516,    -0.006754829380],  #TEST
             [ 0.000000000000,    -0.002462822516,    -0.006754829380]   #TEST
           ]                                                             #TEST
ref = PsiMod.Matrix(3, 3)                                                #TEST
ref.set(ref_vals)

memory 250 mb

molecule h2o {
0 1
o
h 1 0.958
h 1 0.958 2 104.4776 
}

set {
  basis cc-pvdz
}
gradient('omp2')

grad = PsiMod.wavefunction().gradient()                           #TEST
compare_matrices(ref, grad, 6, "Analytic gradients")              #TEST

