
molecule sodium {
1 1
Na
}


activate(sodium)
set basis STO-3G
set reference rhf
set guess core
cation_e = energy('scf')
compare_values(cation_e, -159.7807309902018, 9, "Singlet cation energy")             #TEST

molecule sodium {
0 2
Na
}

activate(sodium)
set basis STO-3G
set reference rohf
set guess read
set docc [ 2, 0, 0, 0, 0, 1, 1, 1 ]
set socc [ 1, 0, 0, 0, 0, 0, 0, 0 ]
neutral_e = energy('scf')
compare_values(neutral_e, -159.6646122680461986, 9, "Doublet neutral energy")         #TEST


molecule {
0 2
Li
}

set basis STO-3G
set reference rohf
set guess sad
set docc [ 1, 0, 0, 0, 0, 0, 0, 0 ]
set socc [ 1, 0, 0, 0, 0, 0, 0, 0 ]
li_e = energy('scf')
compare_values(li_e, -7.3155452344445377, 9, "Lithium doublet energy")             #TEST
